{
  "name": "enter-folder",
  "displayName": "Enter Folder",
  "description": "VSCode extension, into folders as root in Explorer",
  "version": "0.0.1",
  "publisher": "xiangda",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ada87/opensystemshell.git"
  },
  "icon": "icon.png",
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  
  "contributes": {
    "commands": [
      {
        "command": "enterFolder.enterFolder",
        "title": "Enter Folder",
        "icon": "$(arrow-right)"
      },
      {
        "command": "enterFolder.backToParent",
        "title": "Back to Parent",
        "icon": "$(arrow-left)"
      },
      {
        "command": "enterFolder.backToRoot",
        "title": "Back to Root",
        "icon": "$(home)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "enterFolder.enterFolder",
          "when": "explorerResourceIsFolder",
          "group": "2_workspace"
        },
        {
          "command": "enterFolder.backToParent",
          "when": "enterFolder.inSubFolder",
          "group": "2_workspace"
        }
      ],
      "view/title": [
        {
          "command": "enterFolder.backToParent",
          "when": "view =~ /explorer/ && enterFolder.inSubFolder",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "enterFolder.backToParent",
          "when": "enterFolder.inSubFolder"
        },
        {
          "command": "enterFolder.backToRoot"
        }
      ]
    },
    "keybindings": [
      {
        "command": "enterFolder.enterFolder",
        "key": "ctrl+shift+enter",
        "when": "explorerViewletFocus"
      },
      {
        "command": "enterFolder.backToParent",
        "key": "ctrl+shift+backspace",
        "when": "enterFolder.inSubFolder"
      }
    ],
    "configuration": {
      "title": "Enter Folder",
      "properties": {
        "enterFolder.navigationState": {
          "type": "object",
          "description": "Internal navigation state (do not modify manually)",
          "default": null
        }
      }
    }
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "20.x",
    "typescript": "^4.9.4"
  }
}